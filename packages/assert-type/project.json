{
	"name": "assert-type",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/assert-type/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/assert-type",
				"main": "packages/assert-type/src/index.ts",
				"tsConfig": "packages/assert-type/tsconfig.lib.json",
				"assets": ["packages/assert-type/*.md"]
			}
		},
		"publish": {
			"executor": "nx:run-commands",
			"options": {
				"command": "node tools/scripts/publish.mjs assert-type {args.ver} {args.tag}"
			},
			"dependsOn": ["build"]
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/assert-type/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/assert-type/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	},
	"tags": []
}
