{
	"$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
	"bundle": {
		"active": true,
		"targets": "all",
		"windows": {
			"certificateThumbprint": null,
			"digestAlgorithm": "sha256",
			"timestampUrl": ""
		},
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"resources": [],
		"externalBin": [],
		"copyright": "",
		"category": "DeveloperTool",
		"shortDescription": "",
		"longDescription": "",
		"macOS": {
			"frameworks": [],
			"minimumSystemVersion": "",
			"exceptionDomain": "",
			"signingIdentity": null,
			"entitlements": null
		},
		"linux": {
			"deb": {
				"depends": []
			}
		}
	},
	"build": {
		"beforeBuildCommand": "nx run-many --target build --projects=tidy-api,tidy",
		"frontendDist": "../../../dist/apps/tidy",
		"beforeDevCommand": "nx run-many --target serve --projects=tidy-api,tidy",
		"devUrl": "http://localhost:4200"
	},
	"productName": "tidy",
	"mainBinaryName": "tidy",
	"version": "0.1.0",
	"identifier": "com.tauri.dev",
	"plugins": {},
	"app": {
		"security": {
			"csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'; connect-src ipc: http://ipc.localhost"
		},
		"windows": [
			{
				"title": "Tidy",
				"width": 1080,
				"height": 600,
				"minWidth": 640,
				"minHeight": 360,
				"resizable": true,
				"fullscreen": false,
				"decorations": false,
				"transparent": true,
				"useHttpsScheme": true
			}
		]
	}
}
